        <rdfs:comment>This class comprises S20 Physical Features that are either A2 Stratigraphic Volume Units or A3 Stratigraphic Interfaces</rdfs:comment>
        <rdfs:label xml:lang="en">Stratigraphic Unit</rdfs:label>
    </owl:Class>
    


